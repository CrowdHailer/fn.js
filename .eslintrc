{
	"env": {
		"browser": true,
		"node": true,
		"amd": true
	},
	"globals": {
		"fn": true
	},
	"rules": {
		"no-cond-assign": 0,
		"no-console": 2,
		"no-constant-condition": 2,
		"no-comma-dangle": 2,
		"no-control-regex": 2,
		"no-debugger": 2,
		"no-dupe-keys": 2,
		"no-empty": 2,
		"no-empty-class": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 2,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-jsdoc": 2,
		"valid-typeof": 1,

		"block-scoped-var": 2,
		"complexity": 0,
		"consistent-return": 1,
		"curly": [2, "all"],
		"default-case": 2,
		"dot-notation": 2,
		"eqeqeq": [2, "allow-null"],
		"guard-for-in": 2,
		"no-alert": 2,
		"no-caller": 2,
		"no-div-regex": 2,
		"no-else-return": 2,
		"no-empty-label": 2,
		"no-eq-null": 0,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-labels": 2,
		"no-iterator": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-octal": 2,
		"no-octal-escape": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 1,
		"no-unused-expressions": 0,
		"no-warning-comments": 1,
		"no-with": 2,
		"no-yoda": 2,
		"radix": 2,
		"wrap-iife": [2, "inside"],

		"no-global-strict": 0,
		"no-extra-strict": 2,
		"strict": 2,

		"no-catch-shadow": 2,
		"no-delete-var": 2,
		"no-label-var": 2,
		"no-shadow": 0,
		"no-shadow-restricted-names": 2,
		"no-undef": 2,
		"no-undef-init": 2,
		"no-unused-vars": 2,
		"no-use-before-define": 2,

		"handle-callback-err": 2,
		"no-mixed-requires": 2,
		"no-new-require": 2,
		"no-path-concat": 2,
		"no-process-exit": 2,
		"no-restricted-modules": 0,
		"no-sync": 0,
		"brace-style": [2, "1tbs"],
		"camelcase": 2,
		"consistent-this": [0, "context"],
		"func-names": 0,
		"func-style": [2, "expression"],
		"new-cap": 2,
		"new-parens": 2,
		"no-nested-ternary": 2,
		"no-array-constructor": 2,
		"no-lonely-if": 2,
		"no-new-object": 2,
		"no-spaced-func": 2,
		"no-space-before-semi": 2,
		"no-ternary": 0,
		"no-underscore-dangle": 2,
		"no-wrap-func": 2,
		"quotes": [1, "single"],
		"quote-props": 0,
		"semi": [2, "always"],
		"sort-vars": 0,
		"space-after-keywords": "always",
		"space-in-brackets": [2, "always"],
		"space-infix-ops": 2,
		"space-return-throw-case": 2,
		"space-unary-word-ops": 2,
		"max-nested-callbacks": 0,
		"one-var": 0,
		"wrap-regex": 0
	}
}